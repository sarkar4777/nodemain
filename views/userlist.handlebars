        <!-- page script -->
        <script type="text/javascript">
            $('#example1').dynatable({
			  features: {
				paginate: true,
				sort: true,
				recordCount: true,
				perPageSelect: true
			  },
			  dataset: {
				ajax: true,
				ajaxUrl: '/get/user',
				ajaxOnLoad: true,
				records: [],
				perPageDefault: 1,
				perPageOptions: [1,2,3]
			  },
			  params: {
				sorts: 'order',
				page: 'page',
				perPage: 'per_page',
				queryRecordCount: 'queryRecordCount',
				totalRecordCount: 'totalRecordCount'
			  },
			  table: {
				defaultColumnIdStyle: 'underscore',
				columns: [{
					text: 'Full Name',
					dataIndex: 'full_name',
					groupable: false,
					filter: {
						type: 'list'
					}}, 
					{
					text: 'Company',
					dataIndex: 'company',
					groupable: false,
					filter: {
						type: 'list'
					}},
					{
					text: 'User Type',
					dataIndex: 'userrole',
					groupable: false,
					filter: {
						type: 'list'
					}},
					{
					text: 'Country',
					dataIndex: 'country',
					groupable: false,
					filter: {
						type: 'list'
					}}
				],
				headRowSelector: 'thead tr',
				bodyRowSelector: 'tbody tr'
			  },
			  inputs: {
				paginationClass: 'pagination',
				paginationActiveClass: 'active',
				paginationDisabledClass: 'disabled',
				queryEvent: 'blur change',
				recordCountTarget: null,
				recordCountPlacement: 'after',
				paginationLinkTarget: null,
				paginationLinkPlacement: 'after',
				paginationPrev: 'Previous',
				paginationNext: 'Next',
				paginationGap: [1,2,2,1],
				searchTarget: null,
				searchPlacement: 'before',
				perPageTarget: null,
				perPagePlacement: 'before',
				perPageText: 'Show: ',
				recordCountText: 'Showing ',
				processingText: 'Processing...'
			  }
			});
            
        </script>
